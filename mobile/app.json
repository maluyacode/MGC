{
  "expo": {
    "name": "mobile",
    "slug": "mgc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "com.davemerc.mgc",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.davemerc.mgc",
      "publishBundlePath": "android/app/build/outputs/bundle/release/app.aab",
      "publishManifestPath": "android/app/build/outputs/apk/release/app.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "build": {
          "releaseChannel": "default",
          "distribution": "apk"
        },
        "projectId": "752805f6-a745-4e3d-9e3f-bd68dc6fe7b2"
      }
    },
    "owner": "davemerc"
  }
}